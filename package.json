{
  "name": "chromeext",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "zip": "zip -r _chrome.zip .",
    "dev": "DEV=true parcel public/index.html --port 3000",
    "build:parcel": "parcel build public/index.html --no-source-maps -d build/ && cp public/manifest.json build/ && cp -r public/assets build/",
    "publish": "npm run build:parcel  && zip -r build.zip build/"
  },
  "author": "",
  "assetsPath": "public/",
  "license": "ISC",
  "devDependencies": {
    "cssnano": "^4.1.10",
    "typescript": "^4.1.3",
    "uglify-js": "^3.12.6",
    "webpack": "^5.20.2",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^4.2.0"
  }
}
